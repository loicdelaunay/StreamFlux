<% include header %>
<% include navbar %>

<!-- Card -->
<div class="card card-cascade">

    <!-- Card image -->
    <div class="view view-cascade gradient-card-header blue-gradient">

        <!-- Title -->
        <h2 class="card-header-title mb-3">List of processes : </h2>


    </div>

    <!-- Card content -->
    <div class="card-body card-body-cascade text-center">

        <div id="app-process" class="card-group">

            <!--Card-->
            <div class="card mb-4" v-for="process in processes">

                <!--Card content-->
                <div class="card-body">

                    <!--Title-->
                    <h4 class="card-title">{{process.url}}</h4>

                    <!--Text-->
                    <p class="card-text">Informations :.</p>
                    <span class="badge cyan"><i class="fas fa-hdd"></i> Size : {{process.size}}</span>
                    <span class="badge purple "><i class="fas fa-cloud-download-alt"></i> Speed : {{process.speed}}</span>
                    <span class="badge indigo"><i class="fas fa-clock"></i> Recording time : {{process.time}}</span>

                    <!-- Provides extra visual weight and identifies the primary action in a set of buttons -->
                </div>
                <!--Card content-->
            </div>
            <!--Card-->

        </div>

    </div>

</div>

<% include footer %>